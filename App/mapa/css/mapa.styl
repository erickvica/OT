				padding 15px
				text-align center
				h3
					color #fff
					text-align center
					margin 10px 0 25px 10px
					padding 0
					font-size 1.5em
				input[type="text"]
					background #58ABEC
					border 0
					padding 10px
					width 90%
					margin 12px auto
					display block
					appearance none
				input[type="text"]:first-child
					margin-top 20px
				::-webkit-input-placeholder
					color #f1f1f1
				:-moz-placeholder
					color #f1f1f1
				::-moz-placeholder
					color #f1f1f1
				:-ms-input-placeholder
					color #f1f1f1
				.onoffswitch
					position relative
					width 55px
					-webkit-user-select none
					-moz-user-select none
					-ms-user-select none
					margin-left 10px
					.onoffswitch-checkbox
						display none
					.onoffswitch-label
						display block
						overflow visble
						font-size 1.1em
						white-space nowrap
						cursor pointer
						height 20px
						padding 0 0 0 55px
						line-height 20px
						border 0px solid #FFFFFF
						border-radius 30px
						background-color #9E9E9E
						color #ccc
					.onoffswitch-label:before
						content ""
						display block
						width 30px
						margin -5px
						background #FFFFFF
						position absolute
						top 0
						bottom 0
						right 31px
						border-radius 30px
						box-shadow 0 6px 12px 0px #757575
					.onoffswitch-checkbox:checked + .onoffswitch-label
						background-color #295171
						color #fff
					.onoffswitch-checkbox:checked + .onoffswitch-label, .onoffswitch-checkbox:checked + .onoffswitch-label:before
						border-color #42A5F5
					.onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-inner
						margin-left 0
					.onoffswitch-checkbox:checked + .onoffswitch-label:before
						right 0px
						background-color #31DE46
						box-shadow 3px 6px 18px 0px rgba(0, 0, 0, 0.2)
				span
					overflow hidden
					display inline-block
					font-size 1.3em
					padding-bottom 14px
					margin-left 10px
				button
					display block
					margin 10px auto
					background #82B149
					border 0
					padding 10px
