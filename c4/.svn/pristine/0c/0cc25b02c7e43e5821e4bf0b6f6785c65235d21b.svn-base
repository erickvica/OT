@import 'nib'
@import 'nib/gradients'
@import "https://fonts.googleapis.com/css?family=Lato:400,300,700|Raleway:400,700&subset=latin,latin-ext"
@import "thothers.css"

AzulTelmex=#2196f3

html
	body
		color #333
		background #fff
		h1,h2,h3,h4,h5,h6
			font-weight 700
			font-family 'Raleway'
		h1.intro
			margin-top 50px
			text-align center
			font-weight 400
			font-size 1.8em
			line-height 20px
			span
				font-weight 700
		#loading
			position fixed
			top 0
			left 0
			width 100%
			height 100%
			overflow hidden
			margin 0
			padding 0
			background #00bbfa
			z-index 111
			.bokeh
				font-size 100px
				width 1em
				height 1em
				position absolute
				left 50%
				top 50%
				transform translate(-50%,-50%)
				margin 0
				border-radius 50%
				border 0
				list-style none
				li
					position absolute
					width .2em
					height .2em
					border-radius 50%
				li:nth-child(1)
					left 50%
					top 0
					margin 0 0 0 -.1em
					background #fff
					transform-origin 50% 250%
					animation rota 1.13s linear infinite,opa 3.67s ease-in-out infinite alternate
				li:nth-child(2)
					top 50%
					right 0
					margin -.1em 0 0 0
					background #fff
					transform-origin -150% 50%
					animation rota 1.86s linear infinite,opa 4.29s ease-in-out infinite alternate
				li:nth-child(3)
					left 50%
					bottom 0
					margin 0 0 0 -.1em
					background #fff
					transform-origin 50% -150%
					animation rota 1.45s linear infinite,opa 5.12s ease-in-out infinite alternate
				li:nth-child(4)
					top 50%
					left 0
					margin -.1em 0 0 0
					background #fff
					transform-origin 250% 50%
					animation rota 1.72s linear infinite,opa 5.25s ease-in-out infinite alternate
		#loading.opaco
			background rgba(0,0,0,0.5)
			.bokeh
				li
					background #00bbfa !important
		.ui-dialog
			form
				position relative
				overflow hidden
				fieldset
					border 0
					position relative
					overflow hidden
					h3
						font-size 1.5em
						margin 10px 0
						span
							color #ddd
					h4
						color #aaa
					textarea
						border 1px solid #bbb
						width 100%
						height 200px
						overflow auto
						color #444
					button
						padding 10px 25px
						display block
						margin 10px auto 0 auto
						background #fcc30d
						border 0
						color #333
						&:after
							font-family 'FontAwesome'
							display inline-block
							margin-left 10px
							content "\f1d9"
					button.guardando
						&:after
							font-family 'FontAwesome'
							display inline-block
							margin-left 10px
							content "\f110" !important
							animation fa-spin 2s infinite linear
					button.ok
						&:after
							font-family 'FontAwesome'
							display inline-block
							margin-left 10px
							content "\f00c" !important
					button.bad
						&:after
							font-family 'FontAwesome'
							display inline-block
							margin-left 10px
							content "\f00c" !important
					label
						color #888
						input,select
							background #fff
							border 1px solid #bbb
							padding 3px
							color #00bbfa
					.busca
						display none !important
					.busca.c
						display block !important
			form.edUS
				h3
					color #333
					font-size 1.5em
					margin 10px 0 10px 10px
					span
						color #888
				fieldset
					label
						padding 6px 6px 6px 0
						display block
						min-width 510px
						width 100%
						overflow hidden
						text-align right
						margin 0
						input,select
							float right
							width 82%
							margin-left 10px
					button
						&:after
							content "\f277"
					h5
						margin 0
						padding 0
						text-align center
						color #7A7ACA
						display none
						a
							cursor pointer
							cursor:hand;
							&:hover
								color red
					.asignadas
						color #888
						border 1px solid #bbb
						margin 40px 10px
						background #eee
						padding 20px
						overflow hidden
						a
							display inline-block
							margin 3px 10px
							font-size 1.1em
							cursor:hand;
							cursor pointer
							&:hover
								color red
							&:hover:after
								top -10px
								font-size .8em
								display block
								width 17px
								height 17px
								position absolute
								background #f00
								color #fff
								font-family 'FontAwesome'
								content "\f00d"
								right -15px
								border-radius 50%
								text-align center
								line-height 17px
						a.eliminando
							text-decoration underline
							color #44BB3C
				fieldset:nth-child(1)
					button
						&:after
							content "\f0b1"
			form.edUS.arc
				fieldset:nth-child(1)
					display none
				fieldset:nth-child(2)
						margin 10px auto
		#notificaciones
			position fixed
			top 0
			right 0
			z-index 9999
			width 35%
			min-width 370px
			margin 0
			padding 10px
			overflow hidden
			div.tn
				background #4DBCE9
				border 1px solid #26ADE4
				border-radius 8px
				margin 5px
				padding 15px
				box-shadow 0 1px 1px rgba(0,0,0,0.1), inset 0 1px 0 rgba(255,255,255,0.6)
				opacity 0
				animation fadeOut 5s linear forwards
				h2,p
					color #fff
					margin 0
					padding 0
					line-height 1
					text-shadow 0 1px 1px rgba(0,0,0,0.6)
				h2
					font-size 1.5em
				.tn-progress
					width 0
					height 4px
					background rgba(255,255,255,0.2)
					position absolute
					bottom 5px
					left 2%
					border-radius 3px
					box-shadow inset 0 1px 1px rgba(0,0,0,0.05),0 -1px 0 rgba(255,255,255,0.6)
					animation runProgress 4s linear forwards 0.5s
			div.tn.error
				background #E53935
			div.tn.advertencia
				background #CFBE27
		#loginContenedor
			position absolute
			max-width 300px
			margin 0
			padding 20px
			top 50%
			left 50%
			transform translate(-50%,-50%)
			*,*:after,*:before
				box-sizing border-box
				padding 0
				margin 0
			form
				width 300px
				position relative
				border-radius 5px
				box-shadow 0 0 5px rgba(0,0,0,0.1), 0 3px 2px rgba(0,0,0,0.1)
				&:after
					content ""
					display table
					clear both
				p
					width 70%
					float left
					border-radius 5px 0 0 5px
					border 1px solid rgba(255,255,255,.6)
					border-right none
					padding 0
					margin 0
					i
						opacity .3
						position absolute
						top 13px
						right 5px
						font-size 1.7em
					i:last-child
						top 65px
						right 7px
				input[type=text],input[type=password]
					width auto
					height 50px
					padding 0 10px
					margin 0
					border none
					background white
					background rgba(255,255,255,0.2)
					box-shadow inset 0 0 10px rgba(255,255,255,0.5)
					font-weight 300
					text-indent 10px
					color #fff
					text-shadow 0 1px 2px rgba(0,0,0,0.3)
					font-size 20px
				input[type=text]
					border-bottom 1px solid #fff
					border-bottom 1px solid rgba(255,255,255,0.7)
					border-radius 5px 0 0 0
				button
					width 30%
					height 100px
					margin-top 1px
					float left
					position relative
					overflow hidden
					background #055B9E
					background radial-gradient(ellipse at center, #00bbfa 0%, #055B9E 150%)
					border-radius 0 5px 5px 0
					box-shadow inset 0 0 4px rgba(255, 255, 255, 0.7), inset 0 0 0 1px rgba(0, 0, 0, 0.2)
					border none
					border-left 1px solid silver
					cursor pointer
					line-height 102px
					i
						position absolute
						width 100%
						height 100%
						top 0
						left -20px
						font-size 64px
						line-height 109px
						color #5B5086
						opacity 0
						text-shadow 0 1px 0 rgba(255,255,255,0.4)
						transition all 0.2s ease-out
					span
						display block
						color #5B5086
						font-size 20px
						text-shadow 0 1px 0 rgba(255,255,255,0.4)
						transform rotate(-90deg)
						transition all 0.2s linear
						-webkit-backface-visibility hidden
					&:hover i,&:focus i
						opacity 0.5
						left 0
						transition-delay 0.2s
					&:hover span,&:focus span
						opacity 0
						transform rotate(-90deg) translateY(-20px)
		.container
			#gn-menu
				z-index 9
			#wraper
				height 100%
				width auto
				overflow hidden
				margin 60px 0 0 0
				padding 0
				position relative
				#top,#mapa,#generico
					transition all .2s linear
					position absolute
					left 0
					width 100%
					height 50%
					padding 0
					margin 0
				#generico
					z-index 2
					top 0
					left -100%
					.contenedor
						width auto
						height 100%
						padding 15px 2%
						position relative
						overflow auto
						#ctable,#calendar
							display none
							h2
								color #fcc30d
								font-size 2em
						#ctable.s,#calendar.s
							display block
				#top
					top 0
					#midatos,#grafico
						float left
						width 50%
						height 100%
					#grafico
						tspan
							text-shadow none
					#midatos
						.contenedor
							position absolute
							left 50%
							top 50%
							transform translate(-50%,-50%)
							margin 0
							padding 0
							overflow hidden
							width 330px
							.ui,.data
								display inline-block
							.ui
								font-family 'FontAwesome'
								font-size 10em
								color #00bbfa
								padding 2px 0 0 11px
								width 159px
								text-align center
								margin 0
								border 7px solid #9BE6FF
								border-radius 50%
								overflow hidden
								&:before
									content "\f21b"
							.data
								margin 30px 0 0 0
								float right
								.n
									display block
									font-size 3em
									color #333
								.id,.u
									color #bbb
								.r
									display block
									font-size 1.5em
									color #00bbfa
				#mapa
					bottom 0
				#top.open
					left 100%
				#generico.open
					left 0
				#mapa.openB
					transition all .2s linear
					top 100%
				#generico.openB
					height 100%
	body.login
		color #fff
		background #00bbfa
		background linear-gradient(to left,#055B9E,#00bbfa)
@keyframes fadeOut
	0%
		opacity 0
	10%
		opacity 1
	90%
		opacity 1
		transform translateY(0px)
	99%
		opacity 0
		transform translateY(-40px)
	100%
		opacity 0
@keyframes runProgress
	0%
		width 0%
		background rgba(255,255,255,0.2)
	100%
		width 85%
		background rgba(255,255,255,1)
@-webkit-keyframes rota
	from{ }
	to
		transform rotate(360deg)
@keyframes rota
	from{ }
	to
		transform rotate(360deg)

@-webkit-keyframes opa
	0% { }
	12.0%
		opacity 0.80
	19.5%
		opacity 0.88
	37.2%
		opacity 0.64
	40.5%
		opacity 0.52
	52.7%
		opacity 0.69
	60.2%
		opacity 0.60
	66.6%
		opacity 0.52
	70.0%
		opacity 0.63
	79.9%
		opacity 0.60
	84.2%
		opacity 0.75
	91.0%
		opacity 0.87
@keyframes opa
	0% { }
	12.0%
		opacity 0.80
	19.5%
		opacity 0.88
	37.2%
		opacity 0.64
	40.5%
		opacity 0.52
	52.7%
		opacity 0.69
	60.2%
		opacity 0.60
	66.6%
		opacity 0.52
	70.0%
		opacity 0.63
	79.9%
		opacity 0.60
	84.2%
		opacity 0.75
	91.0%
		opacity 0.87


